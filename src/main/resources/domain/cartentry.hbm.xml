<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="domain">
    <class name="MultiCheese" table="CartEntries">
        <id name="id" column="EntryID">
            <generator class="increment"/>
        </id>
        <property name="quantity"/>

        <many-to-one name="cheeseId" class="domain.Cheese" fetch="select">
            <column name="cheeseid" not-null="true" />
        </many-to-one>

        <many-to-one name="cartId" class="domain.Cart" fetch="select">
            <column name="cartid" not-null="true" />
        </many-to-one>
    </class>
</hibernate-mapping>